{
    "name": "grapheneict/laravel-cognito-guard",
    "description": "Laravel authentication guard to validate JSON Web Tokens (JWT) issued by an AWS Cognito User Pool",
    "keywords": [
        "GrapheneICT",
        "laravel",
        "laravel-cognito-guard"
    ],
    "homepage": "https://github.com/GrapheneICT/laravel-cognito-guard",
    "license": "MIT",
    "authors": [
        {
            "name": "Jovan Stojiljkovic",
            "email": "jstojiljkovic941@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "aws/aws-sdk-php": "^3.235",
        "firebase/php-jwt": "^6.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "lcobucci/jwt": "^4.2",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "GrapheneICT\\CognitoGuard\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GrapheneICT\\CognitoGuard\\Services\\CognitoAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
